<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" dividerPositions="0.8869346733668342" maxHeight="-Infinity"
           maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" orientation="VERTICAL" prefHeight="400.0"
           prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="sample.Controller">
    <items>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
            <children>
                <ScrollPane fx:id="scrollPane" hbarPolicy="ALWAYS" prefHeight="1000" prefWidth="1000"
                            vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <content>
                        <Pane fx:id="outputPane" prefHeight="1000.0" prefWidth="2000.0"
                              style="-fx-background-color: #1D0759;">
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                            </padding>
                        </Pane>
                    </content>
                </ScrollPane>
            </children>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"
                    style="-fx-background-color: #1D0759;">
            <children>
                <GridPane layoutX="-6.0" layoutY="-1.0" prefHeight="41.0" prefWidth="598.0"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                    <columnConstraints>
                        <ColumnConstraints halignment="LEFT" hgrow="ALWAYS"/>
                        <ColumnConstraints/>
                        <ColumnConstraints/>
                        <ColumnConstraints/>
                        <ColumnConstraints/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" valignment="CENTER" vgrow="ALWAYS"/>
                    </rowConstraints>
                    <children>
                        <Button fx:id="playButton" contentDisplay="CENTER" mnemonicParsing="false"
                                onMouseClicked="#playGame" prefHeight="31.0" prefWidth="103.0"
                                style="-fx-background-color: #E3DDF6;" text="Play" textAlignment="CENTER"
                                wrapText="true" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                                GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                            <GridPane.margin>
                                <Insets bottom="10.0" top="10.0"/>
                            </GridPane.margin>
                            <font>
                                <Font name="Bell MT Italic" size="24.0"/>
                            </font>
                        </Button>
                        <Button fx:id="populateButton" mnemonicParsing="false" onMouseClicked="#populateGame"
                                prefHeight="45.0" prefWidth="120.0" style="-fx-background-color: #E3DDF6;"
                                text="Populate" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                                GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                            <GridPane.margin>
                                <Insets bottom="10.0" top="10.0"/>
                            </GridPane.margin>
                            <font>
                                <Font name="Bell MT Italic" size="24.0"/>
                            </font>
                        </Button>
                        <Button fx:id="pauseStopButton" mnemonicParsing="false" onMouseClicked="#pauseStopGame"
                                prefHeight="45.0" prefWidth="147.0" style="-fx-background-color: #E3DDF6;"
                                text="Pause/Stop" textAlignment="CENTER" GridPane.columnIndex="2"
                                GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER"
                                GridPane.vgrow="ALWAYS">
                            <GridPane.margin>
                                <Insets bottom="10.0" top="10.0"/>
                            </GridPane.margin>
                            <font>
                                <Font name="Bell MT Italic" size="24.0"/>
                            </font>
                        </Button>
                        <Label prefHeight="29.0" prefWidth="125.0" text="Generation #" textFill="#f8efef"
                               GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                               GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                            <GridPane.margin>
                                <Insets bottom="10.0" top="10.0"/>
                            </GridPane.margin>
                            <font>
                                <Font name="Bell MT Italic" size="24.0"/>
                            </font>
                        </Label>
                        <Label fx:id="generationNumberLabel" contentDisplay="CENTER" text="0" textFill="#f8f1f1"
                               GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                               GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                            <GridPane.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </GridPane.margin>
                            <font>
                                <Font name="Bell MT Italic" size="24.0"/>
                            </font>
                        </Label>
                    </children>
                </GridPane>
            </children>
        </AnchorPane>
    </items>
</SplitPane>
